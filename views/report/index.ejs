<!DOCTYPE html>
<html lang="en">
<head>
	<title>title</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta name="description" content="" />
	<meta name="keywords" content="" />
	<meta name="robots" content="noindex,nofollow" />
	
	<link href="/styles/bootstrap.css" rel="stylesheet" media="screen" />
	<link href="/styles/style.css" rel="stylesheet" media="screen" />
	<script type="text/javascript" src="/js/jq.js"></script>
	<script type="text/javascript" src="/js/bootstrap.min.js"></script>
	<script type="text/javascript" src="/js/report.js"></script>
	<script type="text/javascript" src="/js/query.js"></script>
	
	<!--script type="text/javascript" src="/js/main.js"></script-->
	
</head>

<body>
	<%- partial('../partials/header') %>
	<div style="height:30%;overflow: auto;">
		<h3 style="margin-left:20px;width:25%">Reports</h3>
		<section class="leftpanel">
			<div>Test states
		<hr></hr>
		<a href="#" class="status getQuery" url="status=Pending"><i class="icon-refresh"></i>Pending <span style="float:right"><%= showstatus.pendingCount %></span></a></br></br>
		<a href="#" class="status getQuery"><i class="icon-check"></i>Completed <span style="float:right"><%= showstatus.completedCount %></span></a></br></br>
		<a href="/reports?status=Running" class="status "><i class="icon-refresh"></i>Running <span style="float:right"><%= showstatus.runningCount %></span></a>
		</div>
		<hr></hr>
		<div>Test types
		<hr></hr>
		<a href="#" class="status">A/B Test</a></br></br>
		<a href="#" class="status">Split URL Test</a></br></br>
		</div>
		</section>
	<section id="reportList" style="float:right;width:70%;height:30%">
    <table id = "reportTable" style="border:1px solid #d5d5d5;" "width=50% height=30% class="table table-striped" >
	
    <tr><th>Test Name <a  class="getQuery" url="sort=name"><i class="icon-chevron-up"></i></a></th><th>Status <a href="/reports?sort=status"><i class="icon-chevron-up"></i></a></th><th>Date <a href="/reports?sort=date"><i class="icon-chevron-up"></i></a></th></tr>
        <% data.forEach(function(report)  { %>
			<tr id=<%= 'reportTr'+report.id %> ><td><a href=<%= report.path %> target=_blank><%= report.name %></a></td><td><%= report.status  %> <i class=<%= showstatus[report.status] %>></i></td><td> <%= report.date %></td></tr>
		<% }); %>
    </table>
	</section>
	</div>
	<%- partial('../partials/footer') %>
</body>
</html>
